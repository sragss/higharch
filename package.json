{
  "name": "higharch",
  "version": "1.0.0",
  "description": "Agentic CLI app to help sort folders",
  "type": "module",
  "main": "src/App.tsx",
  "bin": {
    "higharch": "./bin/higharch.js"
  },
  "scripts": {
